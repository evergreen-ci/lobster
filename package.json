{
  "name": "lobster",
  "private": true,
  "engines": {
    "node": ">=8.6"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "decode-uri-component": "^0.2.0",
    "express": "^4.16.3",
    "morgan": "^1.8.2",
    "needle": "^2.2.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.1",
    "react-highlight-words": "^0.12.0",
    "react-list": "^0.8.10",
    "react-redux": "^5.0.7",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-toggle-button": "^2.2.0",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "rmdir": "^1.2.0",
    "string-hash": "^1.1.3",
    "url-search-params-polyfill": "^4.0.1",
    "whatwg-fetch": "^2.0.4",
    "yargs": "^12.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "react-scripts test --testResultsProcessor=./reporter --env=jsdom",
    "test:e2e": "node ./e2e.js",
    "test:coverage": "react-scripts test --env=jsdom --coverage",
    "flow": "flow",
    "lint": "eslint --ext .js,.jsx -- .",
    "lint:fix": "eslint --ext .js,.jsx --fix .",
    "eslint": "eslint --ext .js,.jsx",
    "debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "generate-tasks": "node generate-tasks.js"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.2.0",
    "eslint-config-mongodb-js": "^3.0.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-junit": "^1.0.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-dependencies": "^2.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.78.0",
    "idb.filesystem.js": "0.0.8",
    "jest-junit": "^5.1.0",
    "localstorage-memory": "^1.0.2",
    "node-fetch": "^2.2.0",
    "react-test-renderer": "^16.4.1",
    "redux-logger": "^3.0.6",
    "redux-saga-test-plan": "^3.7.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "sinon": "^6.1.4"
  },
  "jest-junit": {
    "suiteName": "lobster jest tests",
    "output": "build/jest-junit.xml",
    "suiteNameTemplate": "{filename}",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": "-"
  },
  "proxy": {
    "^/build": {
      "target": "https://logkeeper.mongodb.org",
      "changeOrigin": true
    },
    "^/evergreen": {
      "target": "https://evergreen.mongodb.com",
      "changeOrigin": true,
      "pathRewrite": {
        "^/evergreen": "/"
      }
    }
  }
}
