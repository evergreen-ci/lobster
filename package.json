{
  "name": "lobster",
  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "decode-uri-component": "^0.2.0",
    "eslint-plugin-dependencies": "^2.4.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "express": "^4.16.3",
    "morgan": "^1.8.2",
    "needle": "^2.2.1",
    "prop-types": "^15.6.1",
    "react": "^15.6.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^15.6.2",
    "react-highlight-words": "^0.11.0",
    "react-list": "^0.8.8",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.1.1",
    "react-scripts": "1.1.4",
    "react-toggle-button": "^2.2.0",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "rmdir": "^1.2.0",
    "string-hash": "^1.1.3",
    "url-search-params-polyfill": "^4.0.0",
    "whatwg-fetch": "^2.0.4",
    "yargs": "^8.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "react-scripts test --testResultsProcessor=jest-junit --env=jsdom",
    "flow": "flow",
    "lint": "eslint --ext .js,.jsx -- .",
    "eslint": "eslint --ext .js,.jsx",
    "debug": "react-scripts --inspect-brk test --runInBand --env=jsdom",
    "generate-tasks": "node generate-tasks.js"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "^4.19.1",
    "eslint-config-mongodb-js": "^3.0.1",
    "eslint-junit": "^1.0.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-react": "^7.8.2",
    "idb.filesystem.js": "0.0.8",
    "flow-bin": "^0.74.0",
    "jest-junit": "^3.7.0",
    "node-localstorage": "^1.3.1",
    "react-test-renderer": "^15.5.0",
    "sinon": "^6.0.0"
  },
  "jest-junit": {
    "suiteName": "lobster jest tests",
    "output": "build/jest-junit.xml",
    "suiteNameTemplate": "{filename}",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": "-"
  },
  "proxy": {
    "^/build": {
      "target": "https://logkeeper.mongodb.org",
      "changeOrigin": true
    },
    "^/evergreen": {
      "target": "https://evergreen.mongodb.com",
      "changeOrigin": true,
      "pathRewrite": {
        "^/evergreen": "/"
      }
    }
  }
}
