{
  "name": "lobster",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-minify": "^0.2.0",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.2.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.9.0",
    "body-parser": "^1.17.2",
    "browser-history": "^1.0.1",
    "classnames": "^2.2.5",
    "express": "^4.15.3",
    "hadron-react-components": "^1.10.0",
    "http": "0.0.0",
    "https": "^1.0.0",
    "morgan": "^1.8.2",
    "needle": "^2.0.0",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^15.6.2",
    "react-form": "^1.3.0",
    "react-highlight-words": "^0.11.0",
    "react-list": "^0.8.8",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-scripts": "1.0.16",
    "react-toggle-button": "^2.1.0",
    "reflux": "^0.4.1",
    "reflux-state-mixin": "^0.7.0",
    "rmdir": "^1.2.0",
    "string-hash": "^1.1.3",
    "url-search-params": "^0.10.0",
    "yargs": "^8.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build &&  mv ./build ./build.tmp; mkdir -p ./build; mv ./build.tmp ./build/lobster",
    "test": "react-scripts test $JEST_REPORTER --env=jsdom",
    "test-ci": "CI=true JEST_REPORTER=--testResultsProcessor=jest-junit npm test",
    "eject": "react-scripts eject",
    "eslint": "eslint $ESLINT_REPORTER src",
    "eslint-ci": "CI=true ESLINT_REPORTER='-f ./node_modules/eslint-junit/index.js' npm run eslint"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "eslint": "^4.19.1",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-mongodb-js": "^3.0.1",
    "eslint-junit": "^1.0.1",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.7.0",
    "react-hot-loader": "^4.1.3",
    "react-test-renderer": "^15.6.2"
  },
  "eslint-junit": {
    "suiteName": "eslint tests",
    "output": "./lint-junit.xml",
    "classNameTemplate": "{ruleId}",
    "titleTemplate": "{line} : {source}"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "./test-junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "usePathForSuiteName": "true"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
