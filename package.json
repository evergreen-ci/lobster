{
  "name": "lobster",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "hadron-react-components": "^1.14.1",
    "morgan": "^1.8.2",
    "needle": "^2.2.1",
    "prop-types": "^15.6.1",
    "react": "^15.6.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^15.6.2",
    "react-highlight-words": "^0.11.0",
    "react-list": "^0.8.8",
    "react-router-dom": "^4.1.1",
    "react-scripts": "1.1.4",
    "react-toggle-button": "^2.2.0",
    "reflux": "^0.4.1",
    "reflux-state-mixin": "^0.8.0",
    "rmdir": "^1.2.0",
    "string-hash": "^1.1.3",
    "yargs": "^8.0.2"
